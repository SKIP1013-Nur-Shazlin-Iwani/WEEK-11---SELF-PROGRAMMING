package myMethod;

class GetSquare {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (int i = 1; i<=5; i++) {
			int result = getSquare(i);
			System.out.println("Square of " + i + " is: " + result);
		}
	}
	
	public static int getSquare(int x) {
		return x*x;
	}

}
